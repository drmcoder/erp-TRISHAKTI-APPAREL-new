# Staging Environment Configuration
# TSA ERP Work Assignment System

# API Configuration
VITE_API_BASE_URL=https://api-staging.tsa-erp.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-staging-api-key
VITE_FIREBASE_AUTH_DOMAIN=tsa-erp-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=tsa-erp-staging
VITE_FIREBASE_STORAGE_BUCKET=tsa-erp-staging.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-staging-sender-id
VITE_FIREBASE_APP_ID=your-staging-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-staging-measurement-id

# Security Configuration
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_SENTRY=true
VITE_SENTRY_DSN=your-staging-sentry-dsn

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_AI_RECOMMENDATIONS=true

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_CODE_SPLITTING=true
VITE_ENABLE_LAZY_LOADING=true

# Monitoring and Error Reporting
VITE_ERROR_REPORTING_ENDPOINT=https://api-staging.tsa-erp.com/errors
VITE_PERFORMANCE_ENDPOINT=https://api-staging.tsa-erp.com/metrics
VITE_ANALYTICS_ENDPOINT=https://api-staging.tsa-erp.com/analytics

# Application Metadata
VITE_APP_NAME="TSA ERP - Staging"
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION="Work Assignment System for TSA Manufacturing (Staging)"

# Internationalization
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,ne

# Cache Configuration
VITE_CACHE_VERSION=v1-staging
VITE_CACHE_TTL=1800000

# Notification Configuration
VITE_VAPID_PUBLIC_KEY=your-staging-vapid-public-key

# CDN Configuration
VITE_CDN_URL=https://cdn-staging.tsa-erp.com
VITE_STATIC_ASSETS_URL=https://static-staging.tsa-erp.com